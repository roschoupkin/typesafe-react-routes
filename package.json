{
  "private": true,
  "name": "typesafe-react-routes",
  "version": "1.0.0",
  "description": "Inspired by typesafe-react-router. Typesafe routes for react-router",
  "scripts": {
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "test:watch": "jest --watch",
    "test": "jest --watch=false --coverage",
    "build:watch": "tsc --build --watch",
    "build:lib": "tsc --build tsconfig.lib.json",
    "build:es": "tsc --build tsconfig.es.json",
    "build": "npm run build:lib & npm run build:es",
    "prepare": "husky install"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "author": "roschoupkin",
  "license": "UNLICENSED",
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "tslib": "^2.4.1"
  }
}
